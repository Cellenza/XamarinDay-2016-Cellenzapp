<?xml version="1.0" encoding="UTF-8"?>
<ContentPage
	Title="Settings"
	xmlns="http://xamarin.com/schemas/2014/forms"
	xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
	x:Class="Cellenzapp.Forms.Views.SettingsPage"
	BindingContext="{Binding SettingsViewModel, Source={StaticResource ViewModelLocator}}">
	<ContentPage.Content>
		<TableView
			Intent="Settings">
			<TableRoot>
				<TableSection>
					<SwitchCell
						Text="Synchronisation twitter"
						On="{Binding IsTwitterSynchronized, Mode=TwoWay}" />
				</TableSection>
			</TableRoot>
		</TableView>
	</ContentPage.Content>
</ContentPage>